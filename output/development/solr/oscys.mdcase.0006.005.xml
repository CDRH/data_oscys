      <field name="sourceTitle_s">Deposition of Henrietta Wheeler</field>
      <field name="recordType_s">document</field>
      <field name="term_ss">1794-10</field>
      <field name="documentCaseID_ss" update="add">oscys.caseid.0342</field>
